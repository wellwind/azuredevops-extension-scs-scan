{
    "manifestVersion": 1,
    "id": "scs-scan-task",
    "name": "Security Code Scan Task",
    "version": "0.3.9",
    "publisher": "MikeHuang",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ], 
    "homepage": "https://github.com/wellwind/azuredevops-extension-scs-scan",   
    "description": "Add Security Code Scan task to pipeline. Security Code Scan is a static code analyzer for .NET",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
      "default": "scs.png"
    },
    "content": {
      "details": {
        "path": "README.md"
      }
    },
    "files": [
        {
            "path": "buildandreleasetask"
        }
    ],
    "links": {
      "home": {
        "uri": "https://github.com/wellwind/azuredevops-extension-scs-scan"
      }
    },
    "repository": {
      "type": "git",
      "uri": "https://github.com/wellwind/azuredevops-extension-scs-scan"
    },
    "contributions": [
        {
            "id": "scs-scan-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildandreleasetask"
            }
        }
    ]
}